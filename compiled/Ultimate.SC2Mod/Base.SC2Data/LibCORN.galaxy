bool libCORN_gt_CM_UmojanShadowteamDestroyed_Func (bool testConds, bool runActions) {
    // Automatic Variable Declarations
    // Conditions
    if (testConds) {
        if (!((UnitGetType(EventUnit()) == "UmojanGhostAcademy"))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    if ((UnitGroupHasUnit(libCORN_gv_cM_Shadowteam_Ready[EventPlayer()], EventUnit()) == true)) {
        PlayerAddChargeUsed(EventPlayer(), "Abil/UmojanInsertionTeamCreateTeamDummy", 1.0);
        UnitGroupRemove(libCORN_gv_cM_Shadowteam_Ready[EventPlayer()], EventUnit());
    }

    UnitGroupRemove(libCORN_gv_cM_Shadowteam[EventPlayer()], EventUnit());
    return true;
}

