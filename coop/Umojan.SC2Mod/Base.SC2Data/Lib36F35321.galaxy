// Trigger: CM_UmojanZergSupply
//--------------------------------------------------------------------------------------------------
bool lib36F35321_gt_CM_UmojanZergSupply_Func (bool testConds, bool runActions) {
    // Variable Declarations
    fixed lv_realSupplyMax;
    fixed lv_realSupplyConsummed;
    int lv_supplyAvailable;

    // Automatic Variable Declarations
    // Variable Initialization

    // Conditions
    if (testConds) {
        if (!((libCOMI_gf_ActiveCommanderForPlayer(EventPlayer()) == "UmojanCMD"))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    lv_realSupplyMax = (lv_realSupplyMax + IntToFixed(TechTreeUnitCount(EventPlayer(), "Overlord", c_techCountCompleteOnly)) * 8.0);
    lv_realSupplyMax = (lv_realSupplyMax + IntToFixed(TechTreeUnitCount(EventPlayer(), "UmojanArtificialHatchery", c_techCountCompleteOnly)) * 2.0);
    lv_realSupplyMax = (lv_realSupplyMax + IntToFixed(TechTreeUnitCount(EventPlayer(), "UmojanArtificialHive", c_techCountCompleteOnly)) * 2.0);
    lv_realSupplyMax = (lv_realSupplyMax + IntToFixed(TechTreeUnitCount(EventPlayer(), "UmojanArtificialLair", c_techCountCompleteOnly)) * 2.0);
    lv_realSupplyMax = MinF(lv_realSupplyMax, (50.0 + IntToFixed(TechTreeUpgradeCount(EventPlayer(), "MasteryUmojanMaximumZergControl", c_techCountCompleteOnly))));
    lv_realSupplyConsummed = (lv_realSupplyConsummed + IntToFixed(TechTreeUnitCount(EventPlayer(), "Drone", c_techCountQueuedOrBetter)) * 1.0);
    lv_realSupplyConsummed = (lv_realSupplyConsummed + IntToFixed(TechTreeUnitCount(EventPlayer(), "DroneBurrowed", c_techCountQueuedOrBetter)) * 1.0);
    lv_realSupplyConsummed = (lv_realSupplyConsummed + IntToFixed(TechTreeUnitCount(EventPlayer(), "Zergling", c_techCountQueuedOrBetter)) * 0.5);
    lv_realSupplyConsummed = (lv_realSupplyConsummed + IntToFixed(TechTreeUnitCount(EventPlayer(), "ZerglingBurrowed", c_techCountQueuedOrBetter)) * 0.5);
    lv_realSupplyConsummed = (lv_realSupplyConsummed + IntToFixed(TechTreeUnitCount(EventPlayer(), "Hydralisk", c_techCountQueuedOrBetter)) * 2.0);
    lv_realSupplyConsummed = (lv_realSupplyConsummed + IntToFixed(TechTreeUnitCount(EventPlayer(), "HydraliskBurrowed", c_techCountQueuedOrBetter)) * 2.0);
    lv_realSupplyConsummed = (lv_realSupplyConsummed + IntToFixed(TechTreeUnitCount(EventPlayer(), "Mutalisk", c_techCountQueuedOrBetter)) * 2.0);
    lv_realSupplyConsummed = (lv_realSupplyConsummed + IntToFixed(TechTreeUnitCount(EventPlayer(), "LurkerMP", c_techCountQueuedOrBetter)) * 3.0);
    lv_realSupplyConsummed = (lv_realSupplyConsummed + IntToFixed(TechTreeUnitCount(EventPlayer(), "LurkerMPBurrowed", c_techCountQueuedOrBetter)) * 3.0);
    lv_realSupplyConsummed = (lv_realSupplyConsummed + IntToFixed(TechTreeUnitCount(EventPlayer(), "GuardianMP", c_techCountQueuedOrBetter)) * 2.0);
    lv_realSupplyConsummed = (lv_realSupplyConsummed + IntToFixed(TechTreeUnitCount(EventPlayer(), "Ultralisk", c_techCountQueuedOrBetter)) * 6.0);
    lib36F35321_gv_cM_CommanderKerriganZergSupplyMax = TruncI(lv_realSupplyMax);
    lib36F35321_gv_cM_CommanderKerriganZergSupplyConsummed = TruncI(lv_realSupplyConsummed);
