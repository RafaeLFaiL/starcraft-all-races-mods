bool lib36F35321_gt_DUT_CommanderUmojanKerrigan_Func (bool testConds, bool runActions) {
    // Variable Declarations
    playergroup lv_players;

    // Automatic Variable Declarations
    playergroup auto90444C1F_g;
    int auto90444C1F_var;

    // Variable Initialization
    lv_players = libCOMI_gf_PlayersOfCommander("UmojanCMD");

    // Conditions
    if (testConds) {
        if (!((PlayerGroupCount(lv_players) > 0))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    TriggerEnable(lib36F35321_gt_CM_UmojanZergSupply, true);
    auto90444C1F_g = lv_players;
    auto90444C1F_var = -1;
    while (true) {
        auto90444C1F_var = PlayerGroupNextPlayer(auto90444C1F_g, auto90444C1F_var);
        if (auto90444C1F_var < 0) { break; }
        if ((libCOMI_gf_TechUnitAvailable(auto90444C1F_var, "CoopCasterStukov") == true)) {
            libCOMI_gf_CM_CommanderVOAbilityWatch(auto90444C1F_var, SoundLink("StukovCommander_AleksanderReady", -1), "Abil/StukovSummonAleksander", null, null);
            libCOMI_gf_CM_CommanderVOAbilityWatch(auto90444C1F_var, SoundLink("StukovCommander_ApocaliskReady", -1), "Abil/StukovSummonApocalisk", null, null);
            libCOMI_gf_CreateACCaster(auto90444C1F_var, "CoopCasterStukov");
            libCOMI_gf_CU_GPInitCommonCaster(auto90444C1F_var, "CoopCasterStukov", "Coop_GlobalCastingTemplates/ZergCasterPanelTemplate", "Coop_GlobalCastingTemplates/StukovGlobalCommandPanelTemplate", 4, "Cutscenes\\UI_StukovCastingPanel.SC2Cutscene", "");
        }

        if ((libCOMI_gf_TechUnitAvailable(auto90444C1F_var, "UmojanArtificialHatchery") == true)) {
            libCOMI_gf_CreateACStructure(auto90444C1F_var, "UmojanArtificialHatchery");
            PlayerModifyPropertyInt(auto90444C1F_var, c_playerPropCustom, c_playerPropOperSetTo, 2);
        }

    }
    return true;
}

