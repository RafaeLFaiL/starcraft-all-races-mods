include "TriggerLibs/NativeLib"
include "LibUTES"
include "LibNHbr"
include "Lib2772A861"
include "Lib00B4CE4B"

include "LibUTPV_h"

//--------------------------------------------------------------------------------------------------
// Library: UNIT TESTER - PVP
//--------------------------------------------------------------------------------------------------
// External Library Initialization
void libUTPV_InitLibraries () {
    libNtve_InitVariables();
    libUTES_InitVariables();
    libNHbr_InitVariables();
    lib2772A861_InitVariables();
}

// Triggers
//--------------------------------------------------------------------------------------------------
// Trigger: Initialize
//--------------------------------------------------------------------------------------------------
bool libUTPV_gt_Initialize_Func (bool testConds, bool runActions) {
    // Automatic Variable Declarations
    // Actions
    if (!runActions) {
        return true;
    }

    TriggerExecute(lib00B4CE4B_gt_InitializeDragonsRace, true, false);
    TriggerExecute(lib2772A861_gt_InitializeUEDRace, true, false);
    TriggerExecute(libNHbr_gt_InitializeNetworkHybridRace, true, false);
    return true;
}

//--------------------------------------------------------------------------------------------------
void libUTPV_gt_Initialize_Init () {
    libUTPV_gt_Initialize = TriggerCreate("libUTPV_gt_Initialize_Func");
    TriggerAddEventGeneric(libUTPV_gt_Initialize, libUTES_gv_uNITTESTERINITIALIZED);
}

void libUTPV_InitTriggers () {
    libUTPV_gt_Initialize_Init();
}

//--------------------------------------------------------------------------------------------------
// Library Initialization
//--------------------------------------------------------------------------------------------------
bool libUTPV_InitLib_completed = false;

void libUTPV_InitLib () {
    if (libUTPV_InitLib_completed) {
        return;
    }

    libUTPV_InitLib_completed = true;

    libUTPV_InitLibraries();
    libUTPV_InitTriggers();
}

